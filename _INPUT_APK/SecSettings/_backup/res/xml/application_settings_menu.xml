<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/display_settings"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <PreferenceScreen android:title="@string/share_with_contacts" android:key="share_with_contacts">
        <intent android:targetPackage="com.samsung.android.coreapps" android:action="android.intent.action.MAIN" android:targetClass="com.samsung.android.coreapps.easysignup.ui.SettingPreferences" />
        <extra android:name="from_settings" android:value="true" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/finger_air_view_help_information_priview_splanner_title_calendar" android:key="calendar_settings">
        <intent android:action="com.sec.android.intent.calendar.setting">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/call_settings" android:key="call_settings">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.CallFeaturesSetting">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/sync_contacts" android:key="contact_settings">
        <intent android:targetPackage="com.android.contacts" android:action="android.intent.action.MAIN" android:targetClass="com.android.contacts.preference.ContactsPreferenceActivity">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/sync_contacts" android:key="contact_settings_jpn">
        <intent android:targetPackage="com.samsung.contacts" android:action="android.intent.action.MAIN" android:targetClass="com.samsung.contacts.preference.ContactsPreferenceActivity">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/easy_mode_email" android:key="email_settings">
        <intent android:action="com.android.email.activity.setup.GENERAL_SETTINGS">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/finger_air_view_help_information_priview_gallery_title" android:key="gallery_settings">
        <intent android:targetPackage="com.sec.android.gallery3d" android:action="android.intent.action.MAIN" android:targetClass="com.sec.android.gallery3d.settings.AccountSettingActivity">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/smart_scroll_app_web" android:key="browser_settings">
        <intent android:targetPackage="com.sec.android.app.sbrowser" android:action="android.intent.action.MAIN" android:targetClass="com.sec.android.app.sbrowser.preferences.Settings">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/message" android:key="message_settings">
        <intent android:targetPackage="com.android.mms" android:action="android.intent.action.MAIN" android:targetClass="com.android.mms.settings.EntrancePrefActivity">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/finger_air_view_help_information_priview_splanner_title" android:key="splanner_settings">
        <intent android:action="com.sec.android.intent.calendar.setting">
            <extra android:name="from_settings" android:value="true" />
        </intent>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/apps_svoice_settings" android:key="svoice_settings" />
</PreferenceScreen>