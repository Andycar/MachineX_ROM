<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/radio_controls_title" android:key="parent"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <Preference android:title="@string/smart_bonding" android:key="smart_bonding_settings" android:fragment="com.android.settings.smartbonding.SmartBondingSettings" />
    <PreferenceCategory android:title="@string/status_operator" android:key="network_category" />
    <PreferenceScreen android:title="@string/rcs_settings_title" android:key="rcs_settings">
        <intent android:targetPackage="com.samsung.rcs" android:action="com.samsung.rcs.config.VIEW_SETTINGS" android:targetClass="com.samsung.rcs.configs.ServicesSwitch" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/rcs_settings_title_when_partial_branded" android:key="rcs_settings_partial_branded">
        <intent android:targetPackage="com.samsung.rcs" android:action="com.samsung.rcs.config.VIEW_SETTINGS_PARTIAL_BRANDED" android:targetClass="com.samsung.rcs.configs.PartialBrandedServicesSwitch" />
    </PreferenceScreen>
    <CheckBoxPreference android:persistent="false" android:title="@string/airplane_mode" android:key="toggle_airplane" android:summary="@string/airplane_mode_menu_summary" android:disableDependentsState="true" />
    <com.android.settings.AppListPreference android:persistent="false" android:title="@string/sms_application_title" android:key="sms_application" android:summary="" settings:keywords="@string/keywords_more_default_sms_app" />
    <PreferenceScreen android:title="@string/usb_utility_title" android:key="usb_settings" android:summary="@string/usb_utility_summary" android:fragment="com.android.settings.UsbSettings" />
    <com.android.settings.SmartNetworkPreference android:title="@string/smart_network" android:key="button_smart_network_key" android:summary="@string/smart_network_summary" />
    <PreferenceScreen android:title="@string/print_settings" android:key="print_settings" android:fragment="com.android.settings.print.PrintSettingsFragment" />
    <PreferenceScreen android:title="@string/network_settings_title" android:key="mobile_network_settings" android:dependency="toggle_airplane" settings:keywords="@string/keywords_more_mobile_networks">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.MobileNetworkSettings" />
    </PreferenceScreen>
    <Preference android:persistent="false" android:title="@string/manage_mobile_plan_title" android:key="manage_mobile_plan" />
    <PreferenceScreen android:persistent="false" android:title="@string/wifi_tether_checkbox_text" android:key="wifi_ap_settings" android:fragment="com.android.settings.wifi.mobileap.WifiApSettings">
        <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.wifi.mobileap.WifiApSettings" />
    </PreferenceScreen>
    <com.android.settings.SettingsSwitchPreference android:persistent="false" android:title="@string/wifi_tether_checkbox_text" android:key="enable_switch_wifi_ap" android:fragment="com.android.settings.wifi.mobileap.WifiApSettings">
        <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.wifi.mobileap.WifiApSettings" />
    </com.android.settings.SettingsSwitchPreference>
    <PreferenceScreen android:title="@string/auto_power_on_off" android:key="auto_power_on_off_mode" android:fragment="com.android.settings.autopoweronoff.AutoPowerOnOffSettings">
        <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.autopoweronoff.AutoPowerOnOffSettings" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/tether_settings_title_all" android:key="tether_settings" android:summary="@string/tether_settings_summary_all" android:fragment="com.android.settings.TetherSettings" />
    <PreferenceScreen android:title="@string/vpn_settings_title" android:key="vpn_settings" android:summary="@string/vpn_settings_summary" android:fragment="com.android.settings.vpn2.VpnSettings" />
    <PreferenceScreen android:title="@string/vpn_settings_title" android:key="vpn_settings_for_att" android:summary="@string/vpn_settings_summary" android:fragment="com.android.settings.vpn2.VpnSettingsForATT" />
    <PreferenceScreen android:title="@string/eth_radio_ctrl_title" android:key="ethernet_settings" android:fragment="com.android.settings.ethernet.EthernetSettings" />
    <CheckBoxPreference android:persistent="false" android:title="@string/nsd_quick_toggle_title" android:key="toggle_nsd" android:summary="@string/nsd_quick_toggle_summary" />
    <PreferenceScreen android:title="@string/roaming_settings_title" android:key="roaming_settings" android:summary="@string/roaming_settings_summary" android:dependency="toggle_airplane">
        <intent android:action="android.roaming.DATA_ROAMING_SETTINGS_ENHANCED" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/proxy_settings_title" android:key="proxy_settings" android:fragment="com.android.settings.ProxySelector" />
    <PreferenceScreen android:title="@string/cell_broadcast_settings" android:key="cell_broadcast_settings">
        <intent android:targetPackage="com.android.cellbroadcastreceiver" android:action="android.intent.action.MAIN" android:targetClass="com.android.cellbroadcastreceiver.CellBroadcastSettings" />
    </PreferenceScreen>
    <Preference android:persistent="false" android:key="wifi_calling_holder_key" />
    <Preference android:persistent="false" android:key="wfc_settings_holder_key" />
    <PreferenceCategory android:title="@string/transfer_title" android:key="transfer_category">
        <SwitchPreferenceScreen android:title="@string/nfc_quick_toggle_title" android:key="toggle_nfc" android:summary="@string/nfc_quick_toggle_summary" android:fragment="com.android.settings.nfc.NfcSettings" />
        <SwitchPreferenceScreen android:title="@string/s_beam_settings_title" android:key="s_beam_settings" android:summary="@string/sbeam_menu_summary" android:fragment="com.android.settings.nfc.SBeam" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/media_share_title" android:key="media_share_category">
        <SwitchPreferenceScreen android:title="@string/allshare_nearby" android:key="nearby_settings" android:summary="@string/allshare_title_summary" android:fragment="com.android.settings.nearby.NearbySettings" />
        <PreferenceScreen android:title="@string/wfd_settings_title" android:key="screen_mirroring_settings" android:summary="@string/wfd_settings_summary" android:fragment="com.android.settings.wfd.WifiDisplaySettings" />
        <PreferenceScreen android:title="@string/mirror_link_title" android:key="mirror_link_settings" android:summary="@string/mirror_link_summary" android:fragment="com.android.settings.mirrorlink.MirrorLink" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/transfer_title" android:key="transfer_category_vzw">
        <PreferenceScreen android:title="@string/nfc_quick_toggle_title" android:key="nfc_setting_vzw" android:summary="@string/nfc_quick_toggle_summary" android:fragment="com.android.settings.nfc.NfcSettings" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/media_share_title" android:key="media_share_category_vzw">
        <PreferenceScreen android:title="@string/allshare_nearby" android:key="nearby_setting_vzw" android:summary="@string/allshare_title_summary" android:fragment="com.android.settings.nearby.NearbySettings" />
        <PreferenceScreen android:title="@string/wfd_settings_title" android:key="screen_mirroring_settings_vzw" android:summary="@string/wfd_settings_summary" android:fragment="com.android.settings.wfd.WifiDisplaySettings" />
        <PreferenceScreen android:title="@string/mirror_link_title" android:key="mirror_link_settings_vzw" android:summary="@string/mirror_link_summary" android:fragment="com.android.settings.mirrorlink.MirrorLink" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/kies_title" android:key="kies_category">
        <PreferenceScreen android:title="@string/kies_via_wifi" android:key="kies_via_wifi" android:summary="@string/kies_via_wifi_summary">
            <intent android:targetPackage="com.sec.android.app.kieswifi" android:action="android.intent.action.MAIN" android:targetClass="com.sec.android.app.kieswifi.ui.KiesLauncher" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/data_plan_category" android:key="data_plan_category" />
    <Preference android:title="@string/cdma_pay_as_you_go" android:key="pay_as_you_go" />
</PreferenceScreen>