<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/festival_effect_settings_title" android:key="parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference android:key="festival_effect_help" />
    <PreferenceCategory android:title="@string/allowed_functions" android:key="allowed_functions_category" />
    <SwitchPreferenceScreen android:title="Lock screen" android:key="lock_screen_vn" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="lock_screen_vn" />
    </SwitchPreferenceScreen>
    <SwitchPreferenceScreen android:title="Notification panel" android:key="notification_panel_vn" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="notification_panel_vn" />
    </SwitchPreferenceScreen>
    <com.android.settings.SwitchPreferenceScreen android:title="@string/home_lock_screen_title" android:key="home_lock_screen" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="home_lock_screen" />
    </com.android.settings.SwitchPreferenceScreen>
    <com.android.settings.SwitchPreferenceScreen android:title="@string/notification_panel_title" android:key="notification_panel" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="notification_panel" />
    </com.android.settings.SwitchPreferenceScreen>
    <com.android.settings.SwitchPreferenceScreen android:title="@string/weather_widget_title" android:key="weather_widget" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="weather_widget" />
    </com.android.settings.SwitchPreferenceScreen>
    <com.android.settings.SwitchPreferenceScreen android:title="@string/key_melody_title" android:key="key_melody" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="key_melody" />
    </com.android.settings.SwitchPreferenceScreen>
    <PreferenceCategory android:title="@string/allowed_personalization" android:key="allowed_personalization_category" />
    <com.android.settings.SwitchPreferenceScreen android:title="@string/customize_event_title" android:key="customize_event" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="weather_widget" />
    </com.android.settings.SwitchPreferenceScreen>
    <CheckBoxPreference android:title="@string/festival_home" android:key="festival_home" />
    <CheckBoxPreference android:title="@string/festival_lock_screen_effect" android:key="lock_screen_wallpaper" />
    <CheckBoxPreference android:title="@string/festival_event_notification" android:key="event_notification" />
    <CheckBoxPreference android:title="@string/festival_message_effect" android:key="messages_background" />
    <PreferenceCategory android:title="@string/more_category" android:key="more_category" />
    <PreferenceScreen android:title="@string/festival_theme" android:key="festival_theme">
        <intent android:action="com.sec.android.intent.action.THEME_CHOOSER">
            <extra android:name="festivalTheme" android:value="true" />
        </intent>
    </PreferenceScreen>
    <CheckBoxPreference android:title="@string/lock_screen_wallpaper" android:key="lock_screen_wallpaper_j" />
    <CheckBoxPreference android:title="@string/notification_panel_title" android:key="notification_panel_j" />
    <CheckBoxPreference android:title="@string/messages_background" android:key="messages_background_j" />
</PreferenceScreen>