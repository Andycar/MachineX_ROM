    <header android:icon="@drawable/ic_setting_grid_nfc" android:id="@id/nfc_osaifukeitai_setting" android:title="@string/nfc_osaifukeitai_title" android:summary="@string/nfc_quick_toggle_summary" android:fragment="com.android.settings.nfc.NfcOsaifukeitaiSettings" />
