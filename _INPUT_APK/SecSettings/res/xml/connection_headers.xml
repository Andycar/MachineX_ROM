<?xml version="1.0" encoding="utf-8"?>
<preference-headers
  xmlns:android="http://schemas.android.com/apk/res/android">
    <header android:id="@id/setting_search">
        <intent android:targetPackage="com.android.settings" android:targetClass="com.android.settings.search.SearchMain" />
    </header>
    <header android:id="@id/wireless_section" android:title="@string/header_category_network_connections" />
    <header android:icon="@drawable/ic_settings_wifi_calling" android:id="@id/wifi_calling_settings" android:title="@string/wifi_calling_mode_title">
        <intent android:action="com.oem.smartwifisupport.WIFI_CALLING" />
