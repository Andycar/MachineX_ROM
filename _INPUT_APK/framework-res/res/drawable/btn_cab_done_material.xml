<?xml version="1.0" encoding="utf-8"?>
<selector android:autoMirrored="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <color android:color="?colorControlHighlight" />
    </item>
    <item android:state_focused="true" android:state_enabled="true">
        <nine-patch android:src="@drawable/btn_cab_done_mtrl_alpha" android:tint="?colorControlHighlight" />
    </item>
    <item android:state_enabled="true">
        <nine-patch android:src="@drawable/btn_cab_done_mtrl_alpha" android:tint="?colorButtonNormal" />
    </item>
</selector>